package com.classexam;

import java.io.*;
public class CalcEx {

	public static void main(String[] args) throws IOException{
		Calc c = new Calc();
		
		c.input();
		
		int sum = c.cal();
		
		c.prt(sum);
	}
	
	// 처리
	int cal() {
		int sum = 0;
		switch(ch) {
		case '+':
			sum = n1 + n2;
			break;
		case '-':
			sum = n1 - n2;
			break;
		case '*':
			sum = n1 * n2;
			break;
		case '/':
			sum = n1 / n2;
			break;		
		}
		return sum;
	}
	
	void prt(int a) {
		
	}
	
	// 출력
	void prt(int a) {
		System.out.printf("%d %c %d = %d\n", n1, ch, n2, a);
	}
	

}
