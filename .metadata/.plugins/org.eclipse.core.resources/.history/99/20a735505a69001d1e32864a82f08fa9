package com.classex;

import java.util.*;
public class Sungjuk {
	
	int n; // 인원수 입력받을 필드
	
	// 객체 배열 선언
	Record[] re;
	Scanner sc = new Scanner(System.in);
	
	//인원 설정 메소드 
	void set() {
		do {
			System.out.print("인원수 입력(1~100) : ");
			n = sc.nextInt();
		}while(n<1 || n>100);
		
		re = new Record[n]; // 객체 배열 생성
	}
	
}
